generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Post {
  id Int @id @default(autoincrement())
  title String 
  content String
  published Boolean @default(false)
  authorName String?
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt

  @@map("posts")
}
